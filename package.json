{
  "name": "egg-arango",
  "version": "2.10.01",
  "description": "Simple Api template for ArangoDB.",
  "main": "index.js",
  "eggPlugin": {
    "name": "arango"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "arango",
    "ArangoDB",
    "egg-arango",
    "arangojs"
  ],
  "dependencies": {
    "@hapi/joi": "^16.1.7",
    "arangojs": "^6.12.0",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg": "^2.16.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
